{
  "name": "e2e",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm prebuild && pnpm generate-tests && vite build",
    "prebuild": "esbuild sveltex.config.ts --outfile=sveltex.config.js",
    "generate-tests": "node generate-tests.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@nvl/sveltex": "file:../..",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@wooorm/starry-night": "^3.7.0",
    "esbuild": "0.25.2",
    "glob": "^11.0.1",
    "hast-util-find-and-replace": "^5.0.1",
    "hast-util-to-html": "^9.0.5",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.8",
    "mathjax-full": "^3.2.2",
    "micromark": "^4.0.2",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.2.2",
    "svelte": "^5.26.2",
    "svelte-check": "^4.1.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "vite": "^6.2.6"
  }
}
