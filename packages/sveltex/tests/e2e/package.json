{
  "name": "e2e",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm prebuild && pnpm generate-tests && vite build",
    "prebuild": "esbuild sveltex.config.ts --outfile=sveltex.config.js",
    "generate-tests": "node generate-tests.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@nvl/sveltex": "file:../..",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.8.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@wooorm/starry-night": "^3.4.0",
    "esbuild": "0.21.5",
    "glob": "^10.4.2",
    "hast-util-find-and-replace": "^5.0.1",
    "hast-util-to-html": "^9.0.1",
    "highlight.js": "^11.9.0",
    "katex": "^0.16.10",
    "markdown-it": "^14.1.0",
    "marked": "^13.0.1",
    "mathjax-full": "^3.2.2",
    "micromark": "^4.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "shiki": "^1.10.0",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "unified": "^11.0.5",
    "vite": "^5.3.2"
  }
}
