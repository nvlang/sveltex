{
  "$schema": "https://mintlify.com/schema.json",
  "name": "SvelTeX",
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#128297",
    "light": "#fb923c",
    "dark": "#074e5b",
    "background": {
      "light": "#fff",
      "dark": "#0a0a0a"
    }
  },

  "topbarLinks": [
    {
      "name": "GitHub",
      "url": "https://github.com/nvlang/sveltex"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/nvlang/sveltex"
  }
}
